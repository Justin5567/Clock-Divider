#! /usr/bin/vvp
:ivl_version "11.0 (stable)";
:ivl_delay_selection "TYPICAL";
:vpi_time_precision - 12;
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/system.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_sys.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/vhdl_textio.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2005_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/va_math.vpi";
:vpi_module "/usr/lib/x86_64-linux-gnu/ivl/v2009.vpi";
S_0x603804bdfcf0 .scope package, "$unit" "$unit" 2 1;
 .timescale 0 0;
S_0x603804bdfe80 .scope module, "tb_clkDivide4" "tb_clkDivide4" 3 3;
 .timescale -9 -12;
v0x603804c34430_0 .var "clk", 0 0;
v0x603804c344d0_0 .net "clk4", 0 0, v0x603804be17b0_0;  1 drivers
v0x603804c345a0_0 .var/real "high_time", 0 0;
v0x603804c34670_0 .var/real "period", 0 0;
v0x603804c34710_0 .var "rst_n", 0 0;
v0x603804c34800_0 .var/real "time_rise", 0 0;
E_0x603804c1e9d0 .event negedge, v0x603804be17b0_0;
E_0x603804c1e410 .event posedge, v0x603804be17b0_0;
S_0x603804c1d9d0 .scope module, "uut" "clkDivide4" 3 10, 4 1 0, S_0x603804bdfe80;
 .timescale -9 -12;
    .port_info 0 /INPUT 1 "clk";
    .port_info 1 /INPUT 1 "rst_n";
    .port_info 2 /OUTPUT 1 "clk4";
v0x603804be1350_0 .net "clk", 0 0, v0x603804c34430_0;  1 drivers
v0x603804be17b0_0 .var "clk4", 0 0;
v0x603804c34230_0 .var "cnt", 2 0;
v0x603804c342f0_0 .net "rst_n", 0 0, v0x603804c34710_0;  1 drivers
E_0x603804c03f30/0 .event negedge, v0x603804c342f0_0;
E_0x603804c03f30/1 .event posedge, v0x603804be1350_0;
E_0x603804c03f30 .event/or E_0x603804c03f30/0, E_0x603804c03f30/1;
    .scope S_0x603804c1d9d0;
T_0 ;
    %wait E_0x603804c03f30;
    %load/vec4 v0x603804c342f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_0.0, 8;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x603804c34230_0, 0;
    %jmp T_0.1;
T_0.0 ;
    %load/vec4 v0x603804c34230_0;
    %pad/u 32;
    %cmpi/e 3, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 0, 0, 3;
    %assign/vec4 v0x603804c34230_0, 0;
    %jmp T_0.3;
T_0.2 ;
    %load/vec4 v0x603804c34230_0;
    %addi 1, 0, 3;
    %assign/vec4 v0x603804c34230_0, 0;
T_0.3 ;
T_0.1 ;
    %jmp T_0;
    .thread T_0;
    .scope S_0x603804c1d9d0;
T_1 ;
    %wait E_0x603804c03f30;
    %load/vec4 v0x603804c342f0_0;
    %nor/r;
    %flag_set/vec4 8;
    %jmp/0xz  T_1.0, 8;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x603804be17b0_0, 0;
    %jmp T_1.1;
T_1.0 ;
    %load/vec4 v0x603804c34230_0;
    %pad/u 32;
    %cmpi/e 0, 0, 32;
    %flag_mov 8, 4;
    %load/vec4 v0x603804c34230_0;
    %pad/u 32;
    %cmpi/e 1, 0, 32;
    %flag_or 4, 8;
    %jmp/0xz  T_1.2, 4;
    %pushi/vec4 1, 0, 1;
    %assign/vec4 v0x603804be17b0_0, 0;
    %jmp T_1.3;
T_1.2 ;
    %pushi/vec4 0, 0, 1;
    %assign/vec4 v0x603804be17b0_0, 0;
T_1.3 ;
T_1.1 ;
    %jmp T_1;
    .thread T_1;
    .scope S_0x603804bdfe80;
T_2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x603804c34430_0, 0, 1;
T_2.0 ;
    %delay 5000, 0;
    %load/vec4 v0x603804c34430_0;
    %inv;
    %store/vec4 v0x603804c34430_0, 0, 1;
    %jmp T_2.0;
    %end;
    .thread T_2;
    .scope S_0x603804bdfe80;
T_3 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x603804c34710_0, 0, 1;
    %delay 100000, 0;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x603804c34710_0, 0, 1;
    %delay 500000, 0;
    %vpi_call/w 3 33 "$finish" {0 0 0};
    %end;
    .thread T_3;
    .scope S_0x603804bdfe80;
T_4 ;
    %vpi_call/w 3 38 "$dumpfile", "clkDivide4.vcd" {0 0 0};
    %vpi_call/w 3 39 "$dumpvars", 32'sb00000000000000000000000000000000, S_0x603804bdfe80 {0 0 0};
    %end;
    .thread T_4;
    .scope S_0x603804bdfe80;
T_5 ;
    %wait E_0x603804c1e410;
    %load/vec4 v0x603804c34710_0;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.0, 8;
    %pushi/vec4 0, 0, 32;
    %cvt/rv/s;
    %load/real v0x603804c34800_0;
    %cmp/wr;
    %jmp/0xz  T_5.2, 5;
    %vpi_func/r 3 47 "$realtime" {0 0 0};
    %load/real v0x603804c34800_0;
    %sub/wr;
    %store/real v0x603804c34670_0;
    %load/real v0x603804c34670_0;
    %pushi/real 1342177280, 4071; load=40.0000
    %cmp/wr;
    %flag_inv 4;
    %flag_get/vec4 4;
    %flag_set/vec4 8;
    %jmp/0xz  T_5.4, 8;
    %vpi_call/w 3 48 "$display", "Error: Period is %t, expected 40.0", v0x603804c34670_0 {0 0 0};
    %jmp T_5.5;
T_5.4 ;
    %vpi_call/w 3 49 "$display", "Pass: Period is correct: %t", v0x603804c34670_0 {0 0 0};
T_5.5 ;
T_5.2 ;
    %vpi_func/r 3 51 "$realtime" {0 0 0};
    %store/real v0x603804c34800_0;
T_5.0 ;
    %jmp T_5;
    .thread T_5;
    .scope S_0x603804bdfe80;
T_6 ;
    %wait E_0x603804c1e9d0;
    %load/vec4 v0x603804c34710_0;
    %pushi/vec4 0, 0, 32;
    %cvt/rv/s;
    %load/real v0x603804c34800_0;
    %cmp/wr;
    %flag_get/vec4 5;
    %and;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.0, 8;
    %vpi_func/r 3 57 "$realtime" {0 0 0};
    %load/real v0x603804c34800_0;
    %sub/wr;
    %store/real v0x603804c345a0_0;
    %load/real v0x603804c345a0_0;
    %pushi/real 1342177280, 4070; load=20.0000
    %cmp/wr;
    %flag_inv 4;
    %flag_get/vec4 4;
    %flag_set/vec4 8;
    %jmp/0xz  T_6.2, 8;
    %vpi_call/w 3 58 "$display", "Error: High time is %t, expected 20.0", v0x603804c345a0_0 {0 0 0};
    %jmp T_6.3;
T_6.2 ;
    %vpi_call/w 3 59 "$display", "Pass: High time is correct: %t (50%% duty cycle)", v0x603804c345a0_0 {0 0 0};
T_6.3 ;
T_6.0 ;
    %jmp T_6;
    .thread T_6;
# The file index is used to find the file name in the following table.
:file_names 5;
